<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="159.0" prefWidth="267.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mytunes.gui.controller.PlaylistEditViewController">
   <children>
      <VBox layoutX="22.0" layoutY="12.0" prefHeight="134.0" prefWidth="223.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="82.0" prefWidth="302.0">
               <children>
                  <Label text="Name:">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="input" prefHeight="26.0" prefWidth="179.0">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
            <HBox alignment="TOP_RIGHT" prefHeight="42.0" prefWidth="302.0">
               <children>
                  <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#pressCancel" text="Cancel" />
                  <Button fx:id="saveButton" mnemonicParsing="false" onAction="#pressSave" text="Save">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
